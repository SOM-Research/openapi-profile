         point="org.eclipse.uml2.uml.generated_package">
      <profile
            location="pathmap://OPENAPI_PROFILES/openapi.profile.uml#_8zK3wHyGEemaV87q0fd26g"
            uri="http://edu.uoc.som/openapi/profile/1.0">
      </profile>
   </extension>
   <extension
         point="org.eclipse.papyrus.uml.extensionpoints.UMLProfile">
      <profile
            description="OpenAPI profile"
            iconpath="icons/icon.png"
            name="OpenAPI"
            path="pathmap://OPENAPI_PROFILES/openapi.profile.uml">
      </profile>
   </extension>

