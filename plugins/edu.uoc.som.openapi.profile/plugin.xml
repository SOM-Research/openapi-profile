<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated openapi.profile -->
      <package
            uri="http://edu.uoc.som/openapi/profile/1.0"
            class="edu.som.uoc.openapiprofile.OpenapiprofilePackage"
            genModel="resources/openapi.profile.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <!-- @generated openapi.profile -->
